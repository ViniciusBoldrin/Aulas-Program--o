programa {
  funcao inicio() {
    
  inteiro: a, b, soma, subtracao, divisao, produto

  escreva("Digite o valor de a: ")
  leia(a)

  escreva("Digite o valor de b: ")
  leia(b)

  soma= a+b
  subtracao= a-b
  divisao= a/b
  produto= a*b

  escreva("\nSoma= ", soma)
  escreva("\nSubtracao= ", subtracao)
  escreva("\nDivisao= ", divisao)
  escreva("\nProduto= ", produto)
  }
}
